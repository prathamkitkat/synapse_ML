   "source": [
    "# Task 1.3 \n",
    "request_spending = {\n",
    "    \"Mahek\": {\n",
    "        \"balance\": 3000.00,\n",
    "        \"transactions\": [\n",
    "            {\"amount\": -9000.00, \"category\": \"Creatives\"},\n",
    "            {\"amount\": 7000.00, \"category\": \"Sponsor\"},\n",
    "            {\"amount\": -2000.00, \"category\": \"Prize-Money\"}\n",
    "        ]\n",
    "    },\n",
    "    \"Arham\": {\n",
    "        \"balance\": 5000.00,\n",
    "        \"transactions\": [\n",
    "            {\"amount\": 8000.00, \"category\": \"Stalls\"},\n",
    "            {\"amount\": 7500.00, \"category\": \"Seminars\"}\n",
    "        ]\n",
    "    },\n",
    "    \"Unnati\": {\n",
    "        \"balance\": 3500.00,\n",
    "        \"transactions\": [\n",
    "            {\"amount\": -5000.00, \"category\": \"Attraction\"},\n",
    "            {\"amount\": 2500.00, \"category\": \"Promo\"},\n",
    "            {\"amount\": -900.00, \"category\": \"Lighting\"},\n",
    "            {\"amount\": -3000.00, \"category\": \"Games\"}\n",
    "        ]\n",
    "    },\n",
    "    \"Gaurang\": {\n",
    "        \"balance\": 2000.00,\n",
    "        \"transactions\": [\n",
    "            {\"amount\": -1500.00, \"category\": \"Website\"},\n",
    "            {\"amount\": 1000.00, \"category\": \"C2C\"},\n",
    "            {\"amount\": -500.00, \"category\": \"Posters\"}\n",
    "        ]\n",
    "    }\n",
    "}\n",
    "#dictionary to store index no of each category so that we can access ammount with the help of that index no \n",
    "indexArr = {}\n",
    "for list in request_spending.keys():\n",
    "    for index,value in enumerate(request_spending.get(list).get(\"transactions\")) : \n",
    "        #here value is the dictionary containing ammount and category \n",
    "        indexArr[value[\"category\"]] = index\n",
    "\n",
    "\n",
    "\n",
    "def total_spending(request_spending, account_id : str ,category : str) : \n",
    "    return (request_spending.get(account_id).get(\"transactions\")[indexArr[category]].get(\"amount\"))\n",
    "#request_spending.get(account_id).get(\"transactions\") gives us the transactions  list \n",
    "#indexArr[category] returns the index no for that particular category for eg indexArr[\"Creatives\"] gives 0 \n",
    "\n",
    "def account_balance(request_spending,account_id:str) : \n",
    "    finalBalance = request_spending.get(account_id).get(\"balance\")\n",
    "    for money in request_spending.get(account_id).get(\"transactions\"):\n",
    "        finalBalance = finalBalance + money.get(\"amount\")\n",
    "    return finalBalance\n",
    "\n",
    "def money_owed(request_spending,account_id:str) :\n",
    "    paisa = 0\n",
    "    for money in request_spending.get(account_id).get(\"transactions\"):\n",
    "        paisa = paisa + money.get(\"amount\")\n",
    "    return -1 * paisa\n",
    "\"\"\"if someones paisa is negative that means the commitee owes them money and so this function will return positive value and if someones paisa is\n",
    "positive that means they owe the commitee money so it will return negative value \"\"\"\n",
    "\n",
    "#testing the function for different values now \n",
    "\n",
    "mahekSpending = total_spending(request_spending,\"Mahek\",\"Sponsor\")\n",
    "mahekBalance = account_balance(request_spending,\"Mahek\")\n",
    "mahekOwed = money_owed(request_spending,\"Mahek\")\n",
     st = "received" if (mahekSpending>0) else "spend"
     #if her spending is +ve that means she received money so it will print received else it will print spend as she spend money
    "print(\"For Mahek: \")\n",
    "print(f\"Total {st} in sponsors : {mahekSpending}\\nTotal Balance : {mahekBalance}\\nTotal owed : {mahekOwed}\\n\")\n",
    "\n",
    "arhamSpending = total_spending(request_spending,\"Arham\",\"Stalls\")\n",
    "arhamBalance = account_balance(request_spending,\"Arham\")\n",
    "arhamOwed = money_owed(request_spending,\"Arham\")\n",
     st = "received" if (arhamSpending>0) else "spend"
    "print(\"For Arham: \")\n",
    "print(f\"Total {st} in stalls : {arhamSpending}\\nTotal Balance : {arhamBalance}\\nTotal owed : {arhamOwed}\\n\")\n",
    "\n",
    "unnatiSpending = total_spending(request_spending,\"Unnati\",\"Games\")\n",
    "unnatiBalance = account_balance(request_spending,\"Unnati\")\n",
    "unnatiOwed = money_owed(request_spending,\"Unnati\")\n",
     st = "received" if (unnatiSpending>0) else "spend"
    "print(\"For Unnati: \")\n",
    "print(f\"Total {st} in Games : {unnatiSpending}\\nTotal Balance : {unnatiBalance}\\nTotal owed : {unnatiOwed}\")\n"
   ]
  }
